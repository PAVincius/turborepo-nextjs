{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["$GITHUB_ID", "$GITHUB_SECRET", "$PREVIEW_URL_SECRET"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
